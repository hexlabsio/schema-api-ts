{
  "name": "@hexlabs/schema-api-ts",
  "type": "module",
  "description": "Generate AWS Stacks, APIs and Types from OpenAPI",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "bin": {
    "schema-api-ts": "dist/cli.mjs"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs"
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --configPlugin @rollup/plugin-typescript",
    "test": "jest --ci --runInBand --coverage --reporters=default --reporters=jest-junit --passWithNoTests",
    "lint": "eslint **/*.ts",
    "start": "node dist/cli.js generate -h $(pwd)/test/schema-example.ts v=1.0.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest/globals": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint/eslint-plugin",
      "jest"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-non-null-assertion": 0,
      "@typescript-eslint/no-empty-function": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/ban-types": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexlabsio/apigateway-ts.git"
  },
  "author": "hexlabs",
  "bugs": {
    "url": "https://github.com/hexlabsio/schema-api-ts/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "jest": {
    "transform": {
      ".(ts)": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "homepage": "https://github.com/hexlabsio/schema-api-ts#readme",
  "peerDependencies": {
    "@hexlabs/http-api-ts": "^1.1.32",
    "@hexlabs/lambda-api-ts": "^0.1.12"
  },
  "devDependencies": {
    "@hexlabs/http-api-ts": "^1.1.33",
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/aws-lambda": "^8.10.40",
    "@types/jest": "^24.9.1",
    "@types/node": "^18.18.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.1.1",
    "husky": "^3.1.0",
    "jest": "^29.7.0",
    "jest-junit": "^10.0.0",
    "rollup": "^4.1.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^6.2.1",
    "json-schema-to-typescript": "^10.1.4",
    "ts-node": "^10.9.1"
  }
}
